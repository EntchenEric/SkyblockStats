// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model skyblockItem {
  itemID         String  @id @map("_id")
  name           String
  material       String
  tier           String?
  skin           String?
  npc_sell_price Int?
  wiki_link      String?
}

model vanillaTextures {
  id       String @id @default(cuid()) @map("_id")
  material String @unique
  url      String
}

model FurfSkyRebornTextures {
  id       String @id @default(cuid()) @map("_id")
  material String @unique
  url      String
}

model PacksHQTextures {
  id       String @id @default(cuid()) @map("_id")
  material String @unique
  url      String
}

model HypixelPlusTextures {
  id       String @id @default(cuid()) @map("_id")
  material String @unique
  url      String
}

model missingItems{
  id       String @id @default(cuid()) @map("_id")
  material String @unique
  pack      String
}