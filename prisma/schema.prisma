// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model skyblockItem {
    id             String         @id @default(cuid()) @map("_id")
    itemID         String         @unique
    name           String
    category       String
    material       String
    tier           String?
    skin           String?
    npc_sell_price Int?
    wiki_link      String?
}